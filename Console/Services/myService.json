{
    "Name": "mcs_MCS",
    "Image": "mcs:latest",
    "Networks": [
      "mcs_default"
    ],
    "Labels": {
      "createdByMyService": "true"
    },
    "Volumes": [
      {
        "ReadOnly": true,
        "Source": "/var/run/docker.sock",
        "Target": "/var/run/docker.sock"
      }
    ],
    "Ports": [
        {
          "InternalPort": 5000,
          "ExternalPort": 80,
          "Protocol": "tcp"
        }
      ]
  }